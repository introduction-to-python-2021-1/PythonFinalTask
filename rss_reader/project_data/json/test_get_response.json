{
    "http_error": {
        "message": "The server couldn't fulfill the request.\nError code: {code}",
        "string_with_exception_creation": "HTTPError(None, code, None, None, None)",
        "exception_attributes": {
            "code": 404
        }
    },
    "url_error": {
        "message": "Failed to reach a server.\nReason: {reason}",
        "string_with_exception_creation": "URLError(reason)",
